{
  "version": 2,
  "framework": null,  <-- Ini Penting! Mematikan Zero-Config
  "functions": {
    "*.php": {
      "runtime": "vercel-php@0.7.4"
    }
  },
  "routes": [
    {
      "src": "/(.*)\\.php",
      "dest": "/$1.php"
    },
    {
      "src": "/(.*)",
      "dest": "/index.php"
    }
  ]
}
